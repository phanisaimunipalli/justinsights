  "repository": {
    "type": "git",
    "url": "https://github.com/phanisaimunipalli/justinsights.git"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "javascript"
  ],
  "author": "Phani Sai Ram Munipalli",
  "bugs": {
    "url": "https://github.com/phanisaimunipalli/justinsights/issues"
  },
  "homepage": "https://github.com/phanisaimunipalli/justinsights#readme",
  "dependencies": {
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "fast-csv": "^4.3.6"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
